{
  "name": "store",
  "version": "3.3.5",
  "private": true,
  "path": "/store/share/",
  "scripts": {
    "serve": "VUE_APP_ENV=local vue-cli-service serve --port 1234 && webpack-dev-server --open",
    "wx": "VUE_APP_ENV=local HTTPS=true PORT=1234 vue-cli-service serve",
    "build-dev": "rm -rf build/ && VUE_APP_ENV=development vue-cli-service build",
    "build-fat": "rm -rf build/ && VUE_APP_ENV=fat vue-cli-service build",
    "build-rc": "rm -rf build/ && VUE_APP_ENV=prerelease vue-cli-service build",
    "build-prod": "rm -rf build/ && VUE_APP_ENV=production vue-cli-service build",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "node build.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "clipboard": "^2.0.6",
    "clipboard-polyfill": "^2.8.6",
    "components": "git+https://gitlab.jiliguala.com/npm/components.git",
    "core-js": "^3.4.4",
    "dayjs": "^1.8.25",
    "eventemitter3": "^4.0.7",
    "ggr-util": "git+https://gitlab.jiliguala.com/npm/ggr-util.git",
    "jlgldb-track": "git+https://gitlab.jiliguala.com/npm/jlgldb-track.git",
    "lamp": "git+https://gitlab.jiliguala.com/npm/lamp.git",
    "lib-flexible": "^0.3.2",
    "lodash": "^4.17.15",
    "niuwa-util": "git+https://gitlab.jiliguala.com/npm/niuwa-util.git",
    "number-precision": "^1.5.0",
    "pingpp-js": "^2.2.18",
    "postcss-px2rem": "^0.3.0",
    "px2rem-loader": "^0.1.9",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "utils": "git+http://gitlab.jiliguala.com/npm/utils.git",
    "vant": "^2.10.3",
    "vcomp": "git+https://gitlab.jiliguala.com/npm/vcomp.git",
    "vue": "^2.6.10",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/parser": "^7.14.9",
    "@babel/traverse": "^7.14.9",
    "@babel/types": "^7.14.9",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@sentry/webpack-plugin": "^1.14.2",
    "@vue/babel-preset-app": "^4.5.13",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.2.3",
    "fs": "^0.0.1-security",
    "git-revision-webpack-plugin": "^3.0.6",
    "ip": "^1.1.5",
    "lint-staged": "^10.5.4",
    "path": "^0.12.7",
    "prettier": "2.0.4",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "remove-files-webpack-plugin": "^1.4.4",
    "source-map": "^0.7.3",
    "vue-template-compiler": "^2.6.10",
    "webpack-merge": "^5.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-debugger": 0,
      "no-console": 0,
      "semi": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px2rem": {
        "remUnit": 37.5,
        "propList": [
          "*"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "style",
          "refactor",
          "perf",
          "test",
          "docs",
          "merge",
          "revert",
          "build",
          "chore",
          "other"
        ]
      ],
      "subject-case": [
        0
      ]
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "src/**/*.{js, vue}": [
      "vue-cli-service lint"
    ]
  }
}
